<Window x:Class="Raitichan.AdvancedVRChatPictureSorter.Core.Window.Logger.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Raitichan.AdvancedVRChatPictureSorter.Core.Window.Logger"
        mc:Ignorable="d"
        Title="LogWindow" Height="450" Width="800">
	<Window.DataContext>
		<local:LogViewModel></local:LogViewModel>
	</Window.DataContext>
    <Grid>
		<DataGrid Name="LogGrid" ItemsSource="{Binding LogElements}">
			
		</DataGrid>
    </Grid>
</Window>
